{
  "name": "backend-micro-saas",
  "version": "1.0.0",
  "description": "Backend for a Micro SaaS application with user authentication, site monitoring, and analytics",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --detectOpenHandles",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"**/*.{js,json,md}\"",
    "prepare": "npm run lint && npm test",
    "build": "echo 'No build step required'",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/'), 'node_modules', 'junction') } catch (e) {}\""
  },
  "keywords": [
    "micro-saas",
    "backend",
    "express",
    "mongodb",
    "jwt",
    "redis",
    "authentication",
    "analytics"
  ],
  "author": "Yoquelvis Abreu",
  "repository": {
    "type": "git",
    "url": "https://github.com/yoquelvisdev08/backend-micro-saas.git"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "nock": "^14.0.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0"
  }
}
